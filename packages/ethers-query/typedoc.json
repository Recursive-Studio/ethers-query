{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "out": "./docs",
  "name": "ethers-query",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeInternal": true,
  "excludeExternals": true,
  "cleanOutputDir": true,
  "theme": "default",
  "validation": {
    "invalidLink": true,
    "notDocumented": true
  },
  "navigationLinks": {
    "GitHub": "https://github.com/Recursive-Studio/ethers-query"
  },
  "categorizeByGroup": true,
  "categoryOrder": [
    "Core",
    "Context",
    "Hooks",
    "Connectors",
    "*"
  ],
  "sort": ["source-order"],
  "groupOrder": [
    "Classes",
    "Interfaces",
    "Types",
    "Account",
    "Contracts",
    "Provider",
    "Implementations"
  ],
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false,
    "@beta": true
  },
  "searchInComments": true,
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  }
} 